<br>
<h2> Add a new document for: <br><%= @document.data_package.name %> </h2>
<br>
<div class = "new_document" id = "new_document">
<%= form_for @document do |f|%>
    <%= f.label :name, "Name: " %>
    <%= f.text_field :name %>

    <%= f.label :raw_data, "URL: " %>
    <%= f.text_field :raw_data %>

    <%= f.hidden_field :data_package_id%>

    <%= f.submit "Create document" %> 

<% end %>

</div>

<div id="new-doc-response">
	<h5 class="docID" data-id="<%= @document.id %>"><%= @document.id %></h5>

</div>