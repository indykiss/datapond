
<h2> Create a new document for the <%= @document.data_package.name %> data package here </h2>

<%= form_for @document do |f|%>
    <%= f.label :name, "Name: " %>
    <%= f.text_field :name %>

    <%= f.label :raw_data, "Raw Data: " %>
    <%= f.text_field :raw_data %>

    <%= f.hidden_field :data_package_id%>

    <%= f.submit "Create document" %> 

<% end %>

<div id="new-doc-response">
	<h5 class="docID" data-id="<%= @document.id %>"><%= @document.id %></h5>

</div>